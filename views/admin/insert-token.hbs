{{>header}}
<div id="insert-user" style="margin: 100px 100px 100px 0px;">

    <form  style="margin-left: 180px; width: 100%" method="post" action="/admin/insert-token">
        <!--<p>-->
            <div class="form-group">
            <label for="txtTokens" >Token:</label>
           <!-- <input type="text" id="txtToken"  name="txtToken"  width="200px"/>-->
            <textarea rows="4" cols="50" id="txtTokens" name="txtTokenList"></textarea>
            </div>
        <!--</p>-->
        <input type="submit" value="Insert" />

        <br></br>

        <!--  <p>
              <button id="check-token" value="CheckToken">Check</button>
          </p>

          <br></br>

