            <table class="table table-hover">

                <thead>
                <!--  <tr class="bg-success text-center">
                      <td colspan="6"> <a href="add">Them Video</td>
                  </tr>-->
                <tr class="bg-primary">
                    <!--<th>Id</th>-->
                    <th>Token</th>
                    <th>Status</th>
                    <th>Check</th>
                    <th>Result</th>
                    <th>Delete</th>
                </tr>
                </thead>
                <tbody>
                {{#each data }}
                <tr>
                    <td><input value="{{ this.usertoken }}"/></td>
                    <td>{{ this.status }}</td>
                    <td><a href="check/{{item.id}}"><span class="fa fa-edit">Check</span> </a></td>
                    <td>result check</td>
                    <td><a href="delete/{{item.id}}"><span class="fa fa-remove">Delete</span> </a></td>
                </tr>
                {{/each}}

                </tbody>

            </table>
        </div>
    </div>



</div>